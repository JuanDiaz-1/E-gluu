<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nav_detalles">

    <EditText
        android:id="@+id/buscarcodigo"
        android:layout_width="261dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="84dp"
        android:layout_marginEnd="66dp"
        android:layout_marginBottom="531dp"
        android:ems="10"
        android:hint="@string/det_codigo"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editnombreproducto"
        android:layout_width="261dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="70dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="97dp"
        android:hint="@string/up_nombrep"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editdetallesproducto"
        android:layout_width="261dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="69dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="159dp"
        android:hint="@string/up_detallepro"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editdpreciocompra"
        android:layout_width="261dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="70dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="218dp"
        android:hint="@string/up_preciocompra"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editdprecioventa"
        android:layout_width="261dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="73dp"
        android:layout_marginEnd="77dp"
        android:layout_marginBottom="271dp"
        android:hint="@string/up_precioventa"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/editstock"
        android:layout_width="261dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="70dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="327dp"
        android:hint="@string/up_stock"
        android:inputType="textPersonName" />

    <Button
        android:id="@+id/btnupdate"
        android:layout_width="261dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="70dp"
        android:layout_marginEnd="80dp"
        android:layout_marginBottom="34dp"
        android:text="@string/det_actualiza"
        app:icon="@drawable/ic_editar_producto" />

    <Button
        android:id="@+id/butonbuscar"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="90dp"
        android:layout_marginEnd="78dp"
        android:layout_marginBottom="405dp"
        android:text="@string/det_busca"
        app:icon="@drawable/ic_lupa" />

    <TextView
        android:id="@+id/Datos"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/buscarcodigo"
        android:layout_marginBottom="428dp"
        android:text=""
        android:textSize="24sp" />

    <Button
        android:id="@+id/bt_Scan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/butonbuscar"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="20dp"
        android:text="@string/scan_btn"
        app:icon="@drawable/ic_baseline_camera_alt_24"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button"
        android:layout_width="match_parent"
        android:layout_height="9dp"
        android:layout_above="@+id/editstock"
        android:layout_marginBottom="14dp"
        android:text="Button"
        app:backgroundTint="#61D6D6" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="60dp"
        android:layout_height="50dp"
        android:layout_alignStart="@+id/buscarcodigo"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="260dp"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="527dp"
        android:src="@drawable/ic_barcoder" />

</RelativeLayout>