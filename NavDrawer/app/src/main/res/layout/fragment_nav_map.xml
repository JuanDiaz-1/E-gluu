<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nav_map">


    <Button
        android:id="@+id/btnPrueba"
        android:layout_width="wrap_content"
        android:layout_height="82dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="98dp"
        android:layout_marginEnd="121dp"
        android:layout_marginBottom="375dp"
        android:text="@string/map_btn"
        app:icon="@drawable/ic_mapa_proveedores"
        app:iconGravity="end"
        app:iconSize="40dp"></Button>

    <TextView
        android:id="@+id/txtmap"
        android:layout_width="286dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="55dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="473dp"
        android:text="@string/map_txtview"
        android:textColor="#03A9F4"
        android:textSize="25sp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="245dp"
        android:layout_height="307dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="71dp"
        android:layout_marginEnd="94dp"
        android:layout_marginBottom="33dp"
        android:src="@drawable/map_truck" />


</RelativeLayout>